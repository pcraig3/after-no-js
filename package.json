{
  "name": "after-no-js",
  "version": "1.2.0",
  "description": "After.js with React context and browser cookies: a JS-agnostic proof of concept app",
  "homepage": "https://github.com/pcraig3/after-no-js",
  "license": "MIT",
  "author": "Paul Craig",
  "engines": {
    "node": ">=8"
  },
  "now": {
    "alias": "nojs",
    "dotenv": ".env.production",
    "public": true
  },
  "scripts": {
    "dev": "razzle start",
    "build": "razzle build",
    "lint": "node_modules/eslint/bin/eslint.js --ignore-pattern src/routes.js src",
    "test": "razzle test --env=jsdom",
    "start": "NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "@jaredpalmer/after": "latest",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cookie-encrypter": "^1.0.1",
    "cookie-parser": "^1.4.3",
    "emotion": "^9.2.3",
    "emotion-server": "^9.2.3",
    "express": "^4.16.2",
    "hoist-non-react-statics": "^3.0.1",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.6.2",
    "razzle": "^2.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-plugin-emotion": "^9.2.0",
    "eslint": "^5.3.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-react": "^7.9.1",
    "react-emotion": "^9.2.3"
  }
}
